<property>
<code>var XWiki = (function(XWiki){

  XWiki.PageCreator = Class.create({

    initialize: function(element) {
      this.permalinkHolder = element.down("dl.permalink dd");
      this.form = element.down("form.pageCreation");
      if (this.permalinkHolder) {
        var triggerPreview = this.triggerPreview.bindAsEventListener(this);
        this.inputs = this.form.select("input", "select", "textarea");
        this.inputs.each(function(input){
          ['keyup', 'paste', 'cut'].each(function(eventType) {
            input.observe(eventType, triggerPreview);
          }.bind(this));
        });
        var placeHolder = new XWiki.Document("__PLACEHOLDER__", XWiki.currentDocument.space).getURL('view');
        placeHolder = placeHolder.replace(/__PLACEHOLDER__/, "&lt;span class='name'&gt;&lt;/span&gt;");
        this.permalinkHolder.insert(placeHolder);
        this.permalinkHolder.up('dl').removeClassName('hidden');
        this.triggerPreview();
      }
    },

    triggerPreview: function(event) {
      var url = XWiki.currentDocument.getURL('get', this.form.serialize().replace(/xaction=create/, "xaction=preview") + "&amp;xpage=createpageobject");
      var ajx = new Ajax.Request(url, {
        onComplete: this.onPreviewAvailable.bind(this)
      });
    },

    onPreviewAvailable: function(transport) {
      var result = transport.responseJSON;
      this.permalinkHolder.down("span.name").update(result.name);
    }

  });

  return XWiki;

})(XWiki || {});

document.observe("dom:loaded", function(){
  new XWiki.PageCreator($('mainContentArea'));
});

document.observe("xwiki:popup:loaded", function(){
  new XWiki.PageCreator($('popupContent'));
});</code>
